{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@cli/*": ["./cli/*"],
			"@registry/*": ["./registry/*"],
			"@test/*": ["./test/*"]
		}
	},
	"include": [
		"cli/**/*.ts",
		"registry/**/*.ts",
		"registry/**/*.tsx",
		"registry/**/*.d.ts",
		"test/**/*.ts"
	],
	"exclude": ["node_modules", "dist"]
}
